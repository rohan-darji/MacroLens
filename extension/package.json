{
  "name": "macrolens-extension",
  "version": "1.0.0",
  "description": "Chrome extension that displays nutritional information for food products while shopping online",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev-watch.js",
    "build": "tsc && node scripts/build.js && node scripts/copy-popup.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "chrome-extension",
    "nutrition",
    "food",
    "grocery",
    "walmart"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.270",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "chokidar": "^4.0.3",
    "jsdom": "^25.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.4.0",
    "vitest": "^4.0.16"
  }
}
